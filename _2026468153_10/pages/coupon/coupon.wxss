page {
    background: #fff;
}

.title {
    height: 90rpx;
    display: flex;
    align-items: center;
    width: 94%;
    margin: 0 auto;
}

.title>text {
    border-left: 10rpx solid #ffb53b;
    font-size: 35rpx;
    color: #333;
    padding-left: 15rpx;
    font-weight: 700;
}

.share-coupin {
    margin-top: 10rpx;
}

.coupon {
    display: flex;
    border: 2rpx solid #f2f2f2;
    border-radius: 10rpx;
    height: 150rpx;
    width: 94%;
    margin: 0 auto 20rpx;
}

.coupon>.c-left {
    width: 25%;
    justify-content: center;
    font-size: 35rpx;
    font-weight: 700;
}

.coupon>.c-center,.coupon>.c-left {
    display: flex;
    align-items: center;
}

.coupon>.c-center {
    width: 45%;
    flex-wrap: wrap;
}

.coupon>.c-center>view>view {
    font-size: 30rpx;
    color: #333;
}

.coupon>.c-center>view>text {
    font-size: 22rpx;
    color: #999;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
}

.coupon>.c-right {
    display: flex;
    width: 30%;
    align-items: center;
    justify-content: center;
}

.coupon>.c-right>view {
    width: 140rpx;
    height: 50rpx;
    line-height: 50rpx;
    border-radius: 50rpx;
    text-align: center;
    font-size: 22rpx;
    color: #fff;
}

.disabled {
    background: #999;
}

.h-do,.h-do>view {
    text-align: center;
}

.h-do>view {
    color: #999;
    font-size: 28rpx;
}

.h-do>image {
    width: 45rpx;
    height: 45rpx;
}