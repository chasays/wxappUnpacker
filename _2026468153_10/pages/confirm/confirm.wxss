    font-size: 0;
    width: 100%;
    margin: 0 auto;
}

.calendar>.cal-content>.cal-week>view {
    display: inline-block;
    width: 14.285%;
    color: #666;
    font-size: 30rpx;
    text-align: center;
    border-bottom: 2rpx solid #f2f2f2;
    padding: 20rpx 0;
}

