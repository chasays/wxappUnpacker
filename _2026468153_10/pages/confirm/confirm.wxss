    font-size: 0;
    padding: 10rpx 0;
    width: 100%;
    margin: 0 auto;
}

.calendar>.cal-content>.cal-date>view {
    position: relative;
    display: inline-block;
    width: 14.285%;
    color: #666;
    text-align: center;
    vertical-align: middle;
}

