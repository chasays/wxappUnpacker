    z-index: 2;
    left: 5%;
    top: 20%;
}

.calendar>.cal-header>.cal-year {
    width: 90%;
    margin: 0 auto;
    display: flex;
    align-items: center;
    height: 80rpx;
}

.calendar>.cal-header>.cal-month {
    display: flex;
    font-weight: 700;
    color: #333;
    height: 80rpx;
    width: 40%;
    font-size: 50rpx;
    margin: 0 auto;
    justify-content: space-between;
}

