.order-head {
    display: flex;
    justify-content: space-between;
    height: 150rpx;
}

.order-head>view.head-txt {
    width: 80%;
}

.order-head>view.head-txt>view {
    width: 80%;
    margin: 0 auto;
    color: #fff;
}

.order-head>view.head-txt>view>view {
    font-size: 30rpx;
    margin-top: 25rpx;
}

.order-head>view.head-txt>view>.status {
    line-height: 100rpx;
    font-size: 30rpx;
}

.order-head>view.head-txt>view>text {
    font-size: 26rpx;
}

.order-head>view.head-img {
    width: 20%;
}

.order-head>view.head-img>image {
    width: 100rpx;
    margin-top: 20rpx;
}

.order-details-address {
    display: flex;
    justify-content: space-between;
    padding: 20rpx;
    background: #fff;
}

.order-details-address>.address-ico {
    width: 10%;
    font-size: 20rpx;
    line-height: 150rpx;
    color: #666;
    display: flex;
    justify-content: center;
    align-items: center;
}

.order-details-address>.address-ico>image {
    width: 40rpx;
    height: 45rpx;
}

.order-details-address>.address-txt {
    width: 90%;
}

.order-details-address>.address-txt>.h3 {
    display: flex;
    justify-content: space-between;
}

.order-details-address>.address-txt>.h3>text {
    color: #666;
    float: right;
    font-size: 35rpx;
}

.order-details-address>.address-txt>.addres {
    line-height: 50rpx;
    color: #666;
    font-size: 30rpx;
    overflow: hidden;
}

.order-details {
    margin: 15rpx 0;
    background: #fff;
    padding: 15rpx 15rpx 0;
    overflow: hidden;
}

.order-details>.title {
    color: #333;
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 1px solid #f9f9f9;
    display: flex;
    justify-content: space-between;
    font-size: 28rpx;
}

.order-details .box {
    display: flex;
    padding: 15rpx 0;
    border-bottom: 1px solid #eee;
    border-top: 1px solid #eee;
    height: 215rpx;
    overflow: hidden;
    position: relative;
}

.order-details .box .box1 {
    width: 55%;
    padding: .2rem;
}

.order-details .box .box1 image {
    width: 96%;
    height: 200rpx;
    border-radius: 10rpx;
}

.order-details .box .box2 {
    width: 45%;
    padding: 15rpx;
    position: relative;
}

.order-details .box .box2 .name {
    font-size: 30rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: pre-wrap;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.order-details .box .box2 .pos {
    color: #939393;
    text-align: right;
    width: 30%;
}

.order-details .box .box2 .pos .money {
    font-size: 30rpx;
    display: inline-block;
    width: 100%;
}

.order-details .box .box2 .pos .fr {
    float: right;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 30rpx;
}

.order-details .box .box2>.order-btn {
    height: 70rpx;
    display: flex;
    justify-content: flex-end;
    align-items: flex-end;
    position: absolute;
    bottom: 0;
    width: 480rpx;
}

.order-details .box .box2>.order-btn>text {
    font-size: 24rpx;
    border: 2rpx solid #f8b95b;
    color: #f8b95b;
    width: 110rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    border-radius: 50rpx;
}

.order-details>.money {
    height: 80rpx;
    line-height: 80rpx;
    text-align: right;
    color: #333;
}

.order-details>.money>text {
    font-size: 28rpx;
}

.order-details>.money>text.money-size {
    padding-right: 10rpx;
}

.order-details>.money .list-money {
    color: #ffb53b;
    font-size: 32rpx;
}

.attr {
    color: #666;
    font-size: 27rpx;
}

.t-c {
    color: #ff6d12!important;
}

.order-details-ul {
    padding: 0 15rpx;
    background: #fff;
    margin-bottom: 120rpx;
}

.order-details-ul>view {
    display: flex;
    justify-content: space-between;
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 1px solid #f2f2f2;
}

.order-details-ul>view:last-child {
    border-bottom: 0;
}

.order-details-ul>view>text {
    width: 20%;
    text-align: left;
    font-size: 28rpx;
    color: #666;
}

.order-details-ul>view>view {
    width: 80%;
    text-align: right;
    font-size: 28rpx;
    color: #666;
}

.order-details .box .box2>.order-goods {
    display: flex;
    justify-content: space-between;
}

.order-details .box .box2>.order-goods>.goods-txt {
    width: 70%;
}

.foot-order-btn {
    width: 100%;
    padding: 20rpx 0;
    position: fixed;
    bottom: 0;
    display: flex;
    justify-content: flex-end;
    height: 65rpx;
    background: #fff;
}

.foot-order-btn>text {
    width: 150rpx;
    color: #818181;
    font-size: 28rpx;
    text-align: center;
    height: 60rpx;
    line-height: 60rpx;
    border: 1px solid #ccc;
    margin-right: 30rpx;
    border-radius: 30rpx;
}

.foot-order-btn>text.light {
    border: 1px solid #ffb53b;
    color: #ffb53b;
}

.concate {
    display: flex;
    justify-content: space-between;
    height: 100rpx;
    line-height: 100rpx;
    background: #fff;
    margin-top: 20rpx;
}

.concate>button,.concate>view {
    width: 49.5%;
    text-align: center;
    line-height: 100rpx;
}

.concate>button {
    background: #fff;
    border-right: 1px solid #f2f2f2;
}

.concate>button>image,.concate>view>image {
    width: 40rpx;
    height: 40rpx;
    position: relative;
    top: .2rem;
    right: 10rpx;
}

.concate>button>text,.concate>view>text {
    font-size: 28rpx;
}

.msg-layer {
    background: #fff;
    width: 70%;
    height: 330rpx;
    position: fixed;
    top: 30%;
    left: 15%;
    z-index: 3;
    border-radius: 10rpx;
}

.msg-layer>.s-content {
    width: 90%;
    background: #f2f2f2;
    margin: 20rpx auto;
    height: 200rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.msg-layer>.s-content>textarea {
    width: 90%;
    display: block;
    height: 160rpx;
    font-size: 30rpx;
    color: #555;
}

.msg-layer>.s-btn {
    position: absolute;
    border-top: 2rpx solid #f2f2f2;
    bottom: 0;
    height: 90rpx;
    width: 100%;
}

.msg-layer>.s-btn,.msg-layer>.s-btn>view {
    display: flex;
    align-items: center;
}

.msg-layer>.s-btn>view {
    height: 70rpx;
    justify-content: center;
    font-size: 30rpx;
    width: 50%;
}

.msg-layer>.s-btn>view:first-child {
    border-right: 2rpx solid #f2f2f2;
    color: #999;
}

.see-layer {
    background: #fff;
    width: 90%;
    height: 460rpx;
    position: fixed;
    top: 25%;
    left: 5%;
    z-index: 3;
    border-radius: 10rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.sl-close {
    position: absolute;
    width: 50rpx;
    height: 50rpx;
    bottom: -130rpx;
}

.sl-content {
    width: 94%;
    height: 420rpx;
}

.sl-option {
    padding: 20rpx 0;
    line-height: 40rpx;
    font-size: 30rpx;
    color: #555;
    border-bottom: 2rpx solid #f2f2f2;
}

.sl-option>text {
    font-size: 28rpx;
    width: 25%;
    line-height: 40rpx;
    padding-left: 20rpx;
    color: #999;
}